{
  "name": "reactNativeConfigTutorial",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "run-android:base:dev": "react-native run-android --mode=baseDevDebug --appId com.asoftwarefactory.store.app.dev",
    "run-android:base:prod": "react-native run-android --mode=baseProdDebug --appId com.asoftwarefactory.store.app.prod",
    "run-ios:base:dev": "react-native run-ios --scheme \"base dev\"",
    "run-ios:base:prod": "react-native run-ios --scheme \"base prod\"",
    "run-android:coffe:dev": "react-native run-android --mode=coffeDevDebug --appId com.asoftwarefactory.coffe.app.dev",
    "run-android:coffe:prod": "react-native run-android --mode=coffeProdDebug --appId com.asoftwarefactory.coffe.app.prod",
    "run-ios:coffe:dev": "react-native run-ios --scheme \"coffe dev\"",
    "run-ios:coffe:prod": "react-native run-ios --scheme \"coffe prod\""
  },
  "dependencies": {
    "react": "19.0.0",
    "react-native": "0.79.0",
    "react-native-bootsplash": "^6.3.5",
    "react-native-config": "^1.5.5"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "18.0.0",
    "@react-native-community/cli-platform-android": "18.0.0",
    "@react-native-community/cli-platform-ios": "18.0.0",
    "@react-native/babel-preset": "0.79.0",
    "@react-native/eslint-config": "0.79.0",
    "@react-native/metro-config": "0.79.0",
    "@react-native/typescript-config": "0.79.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.0.0",
    "@types/react-test-renderer": "^19.0.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.0.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
